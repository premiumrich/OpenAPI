{
  "name": "trulioo-openapi",
  "version": "1.0.0",
  "scripts": {
    "generate": "openapi-generator-cli batch ./configs/*.yaml",
    "generate:clean": "rm -rf dist/ && npm run generate",
    "postgenerate": "bash scripts/bundle.sh",
    "update-versions": "bash scripts/update-versions.sh",
    "check-versions": "bash scripts/check-versions.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run update-versions && npm run check-versions"
    }
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.1.26",
    "husky": "^4.3.8"
  }
}
